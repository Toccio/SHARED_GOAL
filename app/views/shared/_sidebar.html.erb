<div class=“index-content”>
  <div class=“sidebar”>
  </div>
  <div class=“catecory-cards”>
    <div class=“cards-display”>
      <div class=“cards-titile”><h1><strong>Categories</strong></h1></div>
      <% @lists.each do |list| %>
        <div class=“card”>
          <h2> <%= list.name %> </h2>
          <br>
          <p><strong> <%= list.description %> </strong></p>
          <%= link_to “Check it out”, list_path(list.id), class: “btn”  %>
        </div>
      <% end %>
    </div>
  </div>
</div>
