<div class="main-container">
<%=link_to "Lets go back", lists_path, class: "btn" %>
  <div class="cards">
    <% @classrooms.each do |classroom| %>
      <div class="card">
        <%= classroom.name%>
        <%= link_to "Check it out", "#", class: "btn"  %>
      </div>
    <%end%>
  </div>
</div>

<% @classrooms.each do |classroom| %>
  <%= classroom.name %>
  <%= classroom.description %>
<% end %>
